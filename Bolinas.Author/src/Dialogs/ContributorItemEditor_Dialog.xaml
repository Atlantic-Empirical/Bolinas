<Window x:Class="ContributorItemEditor_Dialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Contributor Item Editor" Height="171" Width="506" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterOwner">
    <Grid>
        <TextBox Height="23" Margin="133,11,19,0" Name="txtReferenceId" VerticalAlignment="Top" TabIndex="0" />
        <Label Height="28" Margin="7,9,0,0" Name="Label1" VerticalAlignment="Top" HorizontalAlignment="Left" Width="120" FontWeight="Bold" HorizontalContentAlignment="Right">ReferenceId:</Label>
        <TextBox Height="23" Margin="133,43,19,0" Name="txtContributorName" VerticalAlignment="Top" TabIndex="1" />
        <Label FontWeight="Bold" Height="28" HorizontalAlignment="Left" Margin="7,41,0,0" Name="Label2" VerticalAlignment="Top" Width="120" HorizontalContentAlignment="Right">Contributor Name:</Label>
        <Button Height="47" Margin="273,72,0,0" Name="btnSave" VerticalAlignment="Top" FontWeight="Bold" FontSize="14" HorizontalAlignment="Left" Width="93" IsDefault="True" TabIndex="2">Save</Button>
        <Button FontSize="14" FontWeight="Bold" Height="47" HorizontalAlignment="Right" Margin="0,72,19,0" Name="btnCancel" VerticalAlignment="Top" Width="93" TabIndex="3">Cancel</Button>
    </Grid>
</Window>
